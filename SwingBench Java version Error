[oracle@node bin]$ ./oewizard -allindexes -bigfile -bs 10000 -cl -create  -dba "admin@mahiatps_high" -dbap "WElcome_12##" -nocompress -p "WElcome_12##" -part -scale 1 -ts ts_sb_oe -u soe21 –v

